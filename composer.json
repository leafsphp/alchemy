{
	"name": "leafs/cli",
	"description": "A simple command line tool for installing and interacting with your leaf apps",
	"homepage": "https://cli.leafphp.dev",
	"version": "v2.0.4",
	"keywords": [
		"leaf",
		"php",
		"installer"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Michael Darko",
			"email": "mickdd22@gmail.com",
			"homepage": "https://mychi.netlify.app",
			"role": "Developer"
		}
	],
	"require": {
		"leafs/fs": "^1.0",
		"psy/psysh": "^0.9.9",
		"symfony/console": "^5.0",
		"symfony/process": "^5.0",
		"phpunit/phpunit": "^9.0",
      	"ext-json": "*"
    },
	"bin": [
		"bin/leaf"
	],
	"autoload": {
		"psr-4": {
			"Leaf\\Alchemy\\": "src/"
		},
		"files": [
			"src/functions.php"
		]
	},
	"config": {
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require-dev": {
		"pestphp/pest": "^1.21"
	}
}
